.orig x3000

;-----------------
; Instructions
;-----------------

	LD R3, DEC_65	; R3 <-- #0
	LD R4, HEX_41	; R4 <-- x41

	HALT		; Halts program
	
;-----------------
; Local Data
;-----------------

	DEC_65	.FILL	#65	; Puts #65 into memory here
	HEX_41	.FILL	x41	; Puts x41 into memory here


.end
	
